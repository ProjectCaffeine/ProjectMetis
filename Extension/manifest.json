{
	"name": "Project Metis",
	"version": "0.0.1",
	"manifest_version": 3,
	"description": "An extension to shut off distracting websites and let you focus on what's important.",
	"permissions": [
		"tabs",
		"activeTab"
	],
	"background": {
		"service_worker": "./ServiceWorker.js",
		"type": "module"
	},
	"action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"content_scripts": [{
    "matches": ["*://*/*"],
    "run_at": "document_start",
    "js": ["./libs/UrlHelperScripts.js"]
  }]
}
